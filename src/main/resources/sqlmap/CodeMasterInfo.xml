<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="codeMasterInfo">

  <!-- 別名 -->
  <typeAlias alias="codeMasterInfo" type="jp.co.sji.edu.fb.common.dto.CodeMasterInfo"/>   
  
  <!-- 全データを検索する -->
  <select id="selectAll" resultClass="codeMasterInfo">
    SELECT
        CODEGROUP,
        CODE,
        CODENAME,
        DISPLAYINDEX
    FROM CODEMASTERINFO
    ORDER BY
        CODEGROUP ASC,
        DISPLAYINDEX ASC
  </select>
  

</sqlMap>